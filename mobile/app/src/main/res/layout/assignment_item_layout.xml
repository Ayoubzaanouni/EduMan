<?xml version="1.0" encoding="utf-8"?>
<!-- assignment_item_layout.xml -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp"
    android:background="#1DB7AE"
    android:padding="16dp">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="63dp"
        android:layout_height="18dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:text="Prof : "
        app:layout_constraintEnd_toStartOf="@+id/textViewAssignmentProfName"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="63dp"
        android:layout_height="18dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:text="Matiere : "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textViewAssignmentProfName"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.042" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="63dp"
        android:layout_height="19dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="36dp"
        android:text="Deadline : "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.024" />

    <TextView
        android:id="@+id/textViewAssignmentTitle"
        android:layout_width="148dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="Assignment Title"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewAssignmentDescription"
        android:layout_width="251dp"
        android:layout_height="50dp"
        android:layout_marginTop="4dp"
        android:text="Assignment Description"
        app:layout_constraintEnd_toEndOf="@+id/textViewAssignmentTitle"
        app:layout_constraintHorizontal_bias="0.359"
        app:layout_constraintStart_toStartOf="@+id/textViewAssignmentTitle"
        app:layout_constraintTop_toBottomOf="@+id/textViewAssignmentTitle" />

    <TextView
        android:id="@+id/textViewAssignmentDeadline"
        android:layout_width="220dp"
        android:layout_height="18dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="16dp"
        android:text="Assignment Deadline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAssignmentDescription" />

    <TextView
        android:id="@+id/textViewAssignmentProfName"
        android:layout_width="219dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:text="Assignment ProfName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAssignmentDeadline" />

    <TextView
        android:id="@+id/textViewAssignmentMatiereName"
        android:layout_width="219dp"
        android:layout_height="19dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:text="Assignment MatiereName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAssignmentProfName" />

    <Button
        android:id="@+id/viewbtn"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:layout_margin="24dp"
        android:text="view"
        app:layout_constraintEnd_toStartOf="@+id/depot"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAssignmentMatiereName" />

    <Button
        android:id="@+id/depot"
        android:layout_width="120dp"
        android:layout_height="48dp"
        android:layout_margin="24dp"
        android:layout_marginTop="24dp"
        android:text="depot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAssignmentMatiereName" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="63dp"
        android:layout_height="19dp"
        android:layout_marginStart="4dp"
        android:text="Desc : "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textViewAssignmentDescription"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.205" />


</androidx.constraintlayout.widget.ConstraintLayout>
